{
  "expo": {
    "scheme": "my-artists",
    "name": "My Artists",
    "slug": "my-artists",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#18191a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "userInterfaceStyle": "dark",
    "ios": {
      "bundleIdentifier": "xilernet.myartists",
      "buildNumber": "1.0.0",
      "supportsTablet": false,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app asks users for their location to automatically find concerts near them. Otherwise, users can manually search for concerts via the google places autocomplete API",
        "LSApplicationQueriesSchemes": [
          "comgooglemaps",
          "applemaps"
        ]
      }
    },
    "android": {
      "package": "xilernet.myartists",
      "versionCode": 1,
      "softwareKeyboardLayoutMode": "pan"
    }
  }
}
