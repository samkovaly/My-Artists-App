{
  "expo": {
    "scheme": "my-artists",
    "description": "My Artists helps you find local concerts of your favorite artists by analyzing your Spotify data. It's the only app that automates this whole process in just one tap. ",
    "name": "My Artists",
    "slug": "my-artists",
    "owner": "samkovaly",
    "privacy": "public",
    "sdkVersion": "38.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#18191a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "userInterfaceStyle": "dark",
    "ios": {
      "bundleIdentifier": "xilernet.myartists",
      "buildNumber": "1.2.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app asks users for their location to automatically find concerts near them. Otherwise, users can manually search for concerts via the google places autocomplete API",
        "LSApplicationQueriesSchemes": [
          "comgooglemaps",
          "applemaps"
        ]
      }
    },
    "android": {
      "package": "xilernet.myartists",
      "versionCode": 3,
      "softwareKeyboardLayoutMode": "pan",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    }
  }
}
